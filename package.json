{
  "name": "medusa-plugin-categories",
  "version": "0.0.1",
  "description": "Product categories modul for Medusajs Admin-UI",
  "author": "PetrH < Nature4.eu >",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Petr-Hl/medusa-plugin-categories"
  },
  "keywords": [
    "typescript",
    "ecommerce",
    "headless",
    "medusa",
    "medusajs",
    "medusa-plugin",
    "medusa-plugin-admin",
    "medusa-plugin-category",
    "medusa-plugin-category-images"
  ],
  "scripts": {
    "clean": "cross-env ./node_modules/.bin/rimraf dist",
    "build": "cross-env npm run clean && npm run build:server && npm run build:admin",
    "build:server": "cross-env npm run clean && tsc -p tsconfig.server.json",
    "build:admin": "cross-env medusa-admin build",
    "prepare": "cross-env NODE_ENV=production yarn build:server && medusa-admin bundle",
    "build:orig": "rm -rf ./dist && tsc -p ./tsconfig.server.json && medusa-admin bundle",
    "watch": "tsc --watch"
  },
  "devDependencies": {
    "@medusajs/admin": "^7.0.0",
    "@medusajs/icons": "^1.0.0",
    "@medusajs/ui": "^1.0.0",
    "@medusajs/medusa": "^1.14.0",
    "@tanstack/react-query": "4.22.0",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.17.7",
    "@types/mime": "1.3.5",
    "@types/node": "^17.0.8",
    "cross-env": "^7.0.3",
    "dotenv": "16.3.1",
    "medusa-interfaces": "1.3.10",
    "react-icons": "^5.2.1",
    "rimraf": "^6.0.1",
    "ts-loader": "^9.5.1",
    "typeorm": "0.3.17",
    "typescript": "4.9.5"
  },
  "peerDependenciesMeta": {
    "medusa-react": {
      "optional": true
    }
  },
  "peerDependencies": {
    "@medusajs/admin": "*",
    "@medusajs/medusa": "^1.x.x",
    "@medusajs/ui": "*",
    "medusa-react": "*"
  },
  "dependencies": {
    "@medusajs/cache-inmemory": "1.8.11",
    "@medusajs/event-bus-local": "1.9.9",
    "@medusajs/file-local": "1.0.4",
    "@medusajs/medusa-cli": "1.3.23",
    "body-parser": "^1.20.2",
    "express": "^4.17.1",
    "medusa-core-utils": "^1.2.0",
    "react-nestable": "^3.0.2",
    "typeorm": "^0.3.17"
  },
  "packageManager": "yarn@1.22.22"
}
